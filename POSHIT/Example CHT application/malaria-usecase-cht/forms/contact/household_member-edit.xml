        </item>
        <item>
          <label ref="jr:itext('/data/household_member/gender/female:label')"/>
          <value>female</value>
        </item>
      </select1>
      <select1 ref="/data/household_member/age_set">
        <label ref="jr:itext('/data/household_member/age_set:label')"/>
