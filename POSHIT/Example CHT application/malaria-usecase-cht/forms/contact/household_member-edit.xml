      <instance>
        <data id="contact:household_member:edit" prefix="J1!contact:household_member:edit!" delimiter="#" version="2023-05-26  9-08">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
            </user>
          </inputs>
          <household_member>
            <parent>PARENT</parent>
            <type>contact</type>
            <contact_type>household_member</contact_type>
            <name/>
            <gender/>
            <age_set/>
            <know_dob/>
            <date_of_birth/>
            <age/>
            <relationship/>
            <specify_other/>
            <screening_note/>
            <meta tag="hidden">
              <created_by/>
              <created_by_person_uuid/>
              <created_by_place_uuid/>
            </meta>
          </household_member>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/inputs" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/household_member/parent" type="string"/>
      <bind nodeset="/data/household_member/type" type="string"/>
      <bind nodeset="/data/household_member/contact_type" type="string"/>
      <bind nodeset="/data/household_member/name" type="string" required="true()"/>
      <bind nodeset="/data/household_member/gender" type="select1" jr:constraintMsg="Please enter a valid local number, or use the standard international format, which includes a plus sign (+) and country code. For example: +254712345678" required="true()" constraint="true()"/>
      <bind nodeset="/data/household_member/age_set" type="select1" jr:constraintMsg="Please enter a valid local number, or use the standard international format, which includes a plus sign (+) and country code. For example: +254712345678" required="true()" constraint="true()"/>
      <bind nodeset="/data/household_member/know_dob" type="select1" relevant="selected(  /data/household_member/age_set ,'over_5')" required="true()"/>
      <bind nodeset="/data/household_member/date_of_birth" type="date" relevant="selected(  /data/household_member/know_dob ,'yes') or selected(  /data/household_member/age_set ,'under_5')" required="true()"/>
      <bind nodeset="/data/household_member/age" type="int" relevant="selected(  /data/household_member/know_dob ,'yes')" required="true()"/>
      <bind nodeset="/data/household_member/relationship" type="select1" required="true()"/>
      <bind nodeset="/data/household_member/specify_other" type="string" relevant="selected(  /data/household_member/relationship ,'other')" required="true()"/>
      <bind nodeset="/data/household_member/screening_note" readonly="true()" type="string"/>
      <bind nodeset="/data/household_member/meta/created_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/household_member/meta/created_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/household_member/meta/created_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/data/inputs">
      <group ref="/data/inputs/user">
