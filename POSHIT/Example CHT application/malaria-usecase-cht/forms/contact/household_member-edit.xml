          <label ref="jr:itext('/data/household_member/age_set/under_5:label')"/>
          <value>under_5</value>
        </item>
        <item>
          <label ref="jr:itext('/data/household_member/age_set/over_5:label')"/>
          <value>over_5</value>
        </item>
      </select1>
      <select1 ref="/data/household_member/know_dob">
        <label ref="jr:itext('/data/household_member/know_dob:label')"/>
        <item>
          <label ref="jr:itext('/data/household_member/know_dob/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/data/household_member/know_dob/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <input ref="/data/household_member/date_of_birth">
        <label ref="jr:itext('/data/household_member/date_of_birth:label')"/>
      </input>
      <input ref="/data/household_member/age">
        <label ref="jr:itext('/data/household_member/age:label')"/>
      </input>
      <select1 ref="/data/household_member/relationship">
        <label ref="jr:itext('/data/household_member/relationship:label')"/>
        <item>
          <label ref="jr:itext('/data/household_member/relationship/spouse:label')"/>
          <value>spouse</value>
