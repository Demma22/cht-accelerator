      </select1>
      <input ref="/data/household_member/date_of_birth_calendar">
        <label ref="jr:itext('/data/household_member/date_of_birth_calendar:label')"/>
      </input>
      <input ref="/data/household_member/age">
        <label ref="jr:itext('/data/household_member/age:label')"/>
      </input>
      <select1 ref="/data/household_member/relationship">
        <label ref="jr:itext('/data/household_member/relationship:label')"/>
        <item>
          <label ref="jr:itext('/data/household_member/relationship/spouse:label')"/>
          <value>spouse</value>
        </item>
        <item>
          <label ref="jr:itext('/data/household_member/relationship/child:label')"/>
          <value>child</value>
        </item>
        <item>
          <label ref="jr:itext('/data/household_member/relationship/grandchild:label')"/>
          <value>grandchild</value>
        </item>
        <item>
          <label ref="jr:itext('/data/household_member/relationship/brother:label')"/>
          <value>brother</value>
        </item>
        <item>
          <label ref="jr:itext('/data/household_member/relationship/sister:label')"/>
          <value>sister</value>
        </item>
        <item>
          <label ref="jr:itext('/data/household_member/relationship/other:label')"/>
          <value>other</value>
        </item>
      </select1>
      <input ref="/data/household_member/specify_other">
        <label ref="jr:itext('/data/household_member/specify_other:label')"/>
      </input>
      <input ref="/data/household_member/screening_note">
        <label ref="jr:itext('/data/household_member/screening_note:label')"/>
      </input>
      <group appearance="hidden" ref="/data/household_member/meta"/>
    </group>
  </h:body>
</h:html>
