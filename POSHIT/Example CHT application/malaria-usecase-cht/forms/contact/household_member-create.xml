      <instance>
        <data id="contact:household_member:create" prefix="J1!contact:household_member:create!" delimiter="#" version="2023-05-26  11-32">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
            </user>
          </inputs>
          <household_member>
            <parent>PARENT</parent>
            <type>contact</type>
            <contact_type>household_member</contact_type>
            <name/>
            <gender/>
            <age_set/>
            <know_dob/>
            <date_of_birth_calendar/>
            <age/>
            <age_months/>
            <ephemeral_months/>
            <ephemeral_years/>
            <dob_approx/>
            <dob_raw/>
            <dob_iso/>
            <date_of_birth/>
            <relationship/>
            <specify_other/>
            <screening_note/>
            <meta tag="hidden">
              <created_by/>
              <created_by_person_uuid/>
              <created_by_place_uuid/>
              <last_edited_by/>
              <last_edited_by_person_uuid/>
              <last_edited_by_place_uuid/>
            </meta>
          </household_member>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/inputs" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/household_member/parent" type="string"/>
      <bind nodeset="/data/household_member/type" type="string"/>
      <bind nodeset="/data/household_member/contact_type" type="string"/>
      <bind nodeset="/data/household_member/name" type="string" required="true()"/>
      <bind nodeset="/data/household_member/gender" type="select1" constraint="true()" jr:constraintMsg="Please enter a valid local number, or use the standard international format, which includes a plus sign (+) and country code. For example: +254712345678" required="true()"/>
      <bind nodeset="/data/household_member/age_set" type="select1" constraint="true()" jr:constraintMsg="Please enter a valid local number, or use the standard international format, which includes a plus sign (+) and country code. For example: +254712345678" required="true()"/>
      <bind nodeset="/data/household_member/know_dob" type="select1" relevant="selected(  /data/household_member/age_set ,'over_5')" required="true()"/>
      <bind nodeset="/data/household_member/date_of_birth_calendar" type="date" relevant="selected(  /data/household_member/know_dob ,'yes') or selected(  /data/household_member/age_set ,'under_5')" required="true()"/>
      <bind nodeset="/data/household_member/age" type="int" relevant="selected(  /data/household_member/know_dob ,'yes')" required="true()"/>
      <bind nodeset="/data/household_member/age_months" type="string" calculate="0"/>
      <bind nodeset="/data/household_member/ephemeral_months" type="string" calculate="if(format-date-time(today(),&quot;%m&quot;) - ../age_months &lt; 0, format-date-time(today(),&quot;%m&quot;) - ../age_months + 12, format-date-time(today(),&quot;%m&quot;) - ../age_months)"/>
      <bind nodeset="/data/household_member/ephemeral_years" type="string" calculate="if(format-date-time(today(),&quot;%m&quot;) - ../age_months &lt; 0, format-date-time(today(),&quot;%Y&quot;) - ../age - 1, format-date-time(today(),&quot;%Y&quot;) -../age)"/>
      <bind nodeset="/data/household_member/dob_approx" type="string" calculate="date(concat(string(../ephemeral_years),'-',if(../ephemeral_months&lt;10, concat('0',string(../ephemeral_months)), ../ephemeral_months),'-',string(format-date-time(today(), &quot;%d&quot;))))"/>
      <bind nodeset="/data/household_member/dob_raw" type="string" calculate="if(../know_dob = 'yes' or ../age_set = 'under_5', ../date_of_birth_calendar,../dob_approx )"/>
      <bind nodeset="/data/household_member/dob_iso" type="string" calculate="format-date-time(decimal-date-time(../dob_raw),&quot;%Y-%m-%d&quot;)"/>
      <bind nodeset="/data/household_member/date_of_birth" type="string" calculate="../dob_iso"/>
      <bind nodeset="/data/household_member/relationship" type="select1" required="true()"/>
      <bind nodeset="/data/household_member/specify_other" type="string" relevant="selected(  /data/household_member/relationship ,'other')" required="true()"/>
      <bind nodeset="/data/household_member/screening_note" readonly="true()" type="string"/>
      <bind nodeset="/data/household_member/meta/created_by" type="string" readonly="true()"/>
      <bind nodeset="/data/household_member/meta/created_by_person_uuid" type="string" readonly="true()"/>
      <bind nodeset="/data/household_member/meta/created_by_place_uuid" type="string"/>
      <bind nodeset="/data/household_member/meta/last_edited_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/household_member/meta/last_edited_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/household_member/meta/last_edited_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group ref="/data/inputs">
      <group ref="/data/inputs/user">
        <input ref="/data/inputs/user/contact_id"/>
        <input ref="/data/inputs/user/facility_id"/>
        <input ref="/data/inputs/user/name"/>
      </group>
    </group>
